package es.crimarde.core.hello;

public enum Status {

	SUCCESS(200,"success");
	
	private int code;
	private String desc;
	
	private Status(int code, String desc){
		this.code = code;
		this.desc = desc;
	}

	public int getCode() {
		return code;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	
}
